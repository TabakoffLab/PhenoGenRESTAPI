        "/downloads/datasets": {
            "get": {
                "summary": "datasets - get a list of available datasets.",
                "operationId": "datasets",
                "tags": ["Datasets"],
                "parameters": [
                    {
                        "name": "genomeVer",
                        "in": "query",
                        "description": "Genome Version - rn6,rn5,mm10",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "string",
                            "default":""
                        }
                    },
                    {
                        "name": "organism",
                        "in": "query",
                        "description": "Organism - Rn,Mm",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "string",
                            "default":"Rn"
                        }
                    },
                    {
                        "name": "tissue",
                        "in": "query",
                        "description": "tissue",
                        "required": false,

    "components": {
        "schemas": {
            "datasets": {
                "type": "object",
                "required": [
                    "dataset_id",
                    "organism",
                    "panel",
                    "description",
                    "created",
                    "tissue",
                    "seq_type"
                ],
                "properties": {
                    "dataset_id": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "organism": {
                        "type": "string"
                    },
                    "panel": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "date_created": {
                        "type": "string"
                    },
                    "tissue": {
                        "type": "string"
                    },
                    "sequenceing_type": {
                        "type": "string"
                    },
                    "genome_version": {
                        "type": "string"
                    }
                }
            },
            "dataset": {
                "type":"object",
                "properties":{
                    "dataset_id": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "organism": {
                        "type": "string"
                    },
                    "panel": {
                        "type": "string"
                    },
                    "description": {
                        "type": "string"
                    },
                    "date_created": {
                        "type": "string"
                    },
                    "tissue": {
                        "type": "string"
                    },
                    "sequenceing_type": {
                        "type": "string"
                    },
                    "genome_version": {
                        "type": "string"
                    },
                    "metaData":{
                        "type":"array",
                        "items":{
                            "type":"object",
                            "properties": {
                                "pipelines":{"type":"array","items":{}},
                                "samples":{"type":"array","items":{}},
                                "protocols":{"type":"array","items":{}}
                            }
                        }
                    },
                    "results":{
                        "type":"array",
                        "items":{
                            "type":"object",
                            "properties": {
                                "resultID":{"type":"integer","format": "int32"},
                                "resultType":{"type":"string"},
                                "genomeVersion":{"type":"string"},
                                "hrdpVersion":{"type":"integer","format": "int"},
                                "dateCreated":{"type":"string"}
                            }
                        }
                    }
                }
            },
            "datasetFiles":{
                "type":"array",
                "items":{
                    "$ref": "#/components/schemas/datasetFile"
                }
            },
            "datasetFile":{
                "type":"object",
                "properties": {
                    "fileID":{"type":"integer","format":"int32"},
                    "uploadedDate":{"type":"string"},
                    "fileName":{"type":"string"},
                    "URL":{"type":"string"},
                    "checksum":{"type":"string"},
                    "genomeVersion":{"type":"string"},
                    "description":{"type":"string"},
                    "annotation":{"type":"string"},
                    "level":{"type":"string"},
                    "strainMeans":{"type":"boolean"}
                }

            },
            "TrackData": {
                "type": "object",
                "required": [
                    "id",
                    "name"
                ],
                "properties": {
                    "id": {
                        "type": "integer",
                        "format": "int64"
                    },
                    "name": {
                        "type": "string"
                    },
                    "tag": {
                        "type": "string"
                    }
                }
            },
            "Error": {
                "type": "object",
                "required": [
                    "code",
                    "message"
                ],
                "properties": {
                    "code": {
                        "type": "integer",
                        "format": "int32"
                    },
                    "message": {
                        "type": "string"
                    }
                }
            }
        }
    }

}
    "paths": {
        "/genomeBrowser/trackData": {
            "get": {
                "summary": "trackData - get gene/region data.",
                "description": "Get data for a specific browser track from a specific region or for a gene.",
                "operationId": "trackData",
                "tags": [
                    "trackData"
                ],
                "parameters": [
                    {
                        "name": "trackID",
                        "in": "query",
                        "description": "ID of the track",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "string"
                        }
                    },
                    {
                        "name": "genomeVer",
                        "in": "query",
                        "description": "Genome Version",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "string",
                            "default":"rn6"
                        }
                    },
                    {
                        "name": "chromosome",
                        "in": "query",
                        "description": "Chromosome Location",
                        "required": true,
                        "schema": {
                            "type": "string",
                            "format": "string"
                        }
                    },
                    {
                        "name": "minCoord",
                        "in": "query",
                        "description": "Minimum coordinate",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int32"
                        }
                    },
                    {
                        "name": "maxCoord",
                        "in": "query",
                        "description": "Maximum coordinate",
                        "required": true,
                        "schema": {
                            "type": "integer",
                            "format": "int32"
                        }
                    },
                    {
                        "name": "binSize",
                        "in": "query",
                        "description": "Bin Size for binned data",
                        "required": false,
                        "schema": {
                            "type": "integer",
                            "format": "int32"
                        }
                    },
                    {
                        "name": "version",
                        "in": "query",
                        "description": "Data Version(optional) - allows you to select previous versions of data otherwise the most recent version is used.",
                        "required": false,
                        "schema": {
                            "type": "string",
                            "format": "string",
                            "default":""
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "JSON track data",
                        "headers": {
                            "x-next": {
                                "description": "A link to the next page of responses",
                                "schema": {
                                    "type": "string"
                                }
                            }
                        },
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/TrackData"
                                }
                            }
                        }
                    }
                }
            }
        },
